LengthInt := 60;
Difficulty := 60;
Length := 0.25e-6 * LengthInt;
Nx := 500;
Ny := 32;
Nz := 8;
sizeX := Length + 16e-6;
sizeY := 5e-6;
sizeZ := 20e-6;
