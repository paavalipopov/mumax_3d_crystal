m = uniform(0, 1, 0);
alpha = 0.002;
B_ext = vector(0, 2100e-4, 0);
relax();
saveas(CropY(CropZ(B_eff, Nz/2, Nz/2+1), Ny/2, Ny/2+1), "H_eff");
saveas(CropY(CropZ(B_demag, Nz/2, Nz/2+1), Ny/2, Ny/2+1), "H_demag");
saveas(CropY(CropZ(B_ext, Nz/2, Nz/2+1), Ny/2, Ny/2+1), "H_ext");
