LengthInt := 60;
Difficulty := 60;
Length := 0.25e-6 * LengthInt;
Nx := 160;
Ny := 16;
Nz := 8;
sizeX := Length + 6e-6;
sizeY := 3e-6;
sizeZ := 20e-6;
SetGridSize(Nx, Ny, Nz);
SetCellSize(sizeX/Nx, sizeY/Ny, sizeZ/Nz);
strip := cuboid(Length, 1e-6, sizeZ);
setgeom(strip);
DefRegion(1, strip);
Msat = 1.39e4;
m = uniform(1, 0, 0);
alpha = 0.002;
B_ext = vector(2100e-4, 0, 0);
relax();
saveas(CropY(CropZ(B_eff, Nz/2, Nz/2+1), Ny/2, Ny/2+1), "H_eff");
saveas(CropY(CropZ(B_demag, Nz/2, Nz/2+1), Ny/2, Ny/2+1), "H_demag");
saveas(CropY(CropZ(B_ext, Nz/2, Nz/2+1), Ny/2, Ny/2+1), "H_ext");
